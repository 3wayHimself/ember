      <label x="10" y="10" w="200" h="30" data="Initial State" />
      <panel x="550" y="0" w="90" h="90" bg="logo" />
      <console x="10" y="300" w="620" h="120" />
      <button x="535" y="450" w="100" h="25" standard="qstandard" pressed="qpressed" hilight="qhilight" onClicked="quit" />
      <textbox x="13" y="43" w="97" h="32" data="Text Box!" />
