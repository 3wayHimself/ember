<?xml version = '1.0' encoding = 'UTF-8'?>
<model name="chicken" rotation="90" >
  <submodels>
    <submodel mesh="Chicken.mesh">
      <parts>
        <part name="Chicken" show="true" />
      </parts>
    </submodel>
  </submodels>
  <animations>
  	<animation name="idle">
		<animationpart name="WingFold" weight="0.5" />
		<animationpart name="Peck1" weight="0.5" />
	</animation>
  	<animation name="walk">
		<animationpart name="WingFold" weight="0.5" />
		<animationpart name="Walk" weight="0.5" />
	</animation>
  </animations>
</model>
