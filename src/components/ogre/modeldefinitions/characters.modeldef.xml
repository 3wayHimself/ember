<models>

	<model name="settler" usescaleof="height" >
		<submodels>
			<submodel mesh="3d_skeletons/alpha_male/models/alpha_male.mesh" >
				<parts>
					<part name="Body" show="true" >
						<subentities>
							<subentity name="body" />
						</subentities>
					</part>      
					<part name="Eyes" show="true" >
						<subentities>
							<subentity name="eyes" />
						</subentities>
					</part>      
					<part name="Head" show="true" >
						<subentities>
							<subentity name="head" />
						</subentities>
					</part>
					<part name="Hair" show="true" >
						<subentities>
							<subentity name="hair" />
						</subentities>
					</part>
					<part name="Dress1" show="false" >
						<subentities>
							<subentity name="dress1" material="/global/clothes/dress/sun" />
						</subentities>
					</part>
					<part name="Dress2" show="true" >
						<subentities>
							<subentity name="dress2" material="/global/clothes/dress/sun" />
						</subentities>
					</part>
					<part name="Boots" show="true" >
						<subentities>
							<subentity name="boots" />
						</subentities>
					</part>  
				</parts>      
			</submodel>
		</submodels>
		<Actions>
			<Action name="idle">
				<AnimationParts>
					<animationpart name="_ildeBreathing.BAKED" weight="1" />
				</AnimationParts>
			</Action>
			<Action name="walk">
				<AnimationParts>
					<animationpart name="_walk.BAKED" weight="1" />
				</AnimationParts>
				<sound repeat="true" name="soundfx/creatures/footsteps/Footsteps_on_grass_sasquatch.wav" />
			</Action>
		</Actions>
	</model>	

	<model name="_settler" usescaleof="height" >
