<?xml version = '1.0' encoding = 'UTF-8'?>
<models>
	<model name="birch" usescaleof="height" scale="2" renderingdistance="200" >
		<submodels>
			<submodel mesh="3d_objects/plants/tree/models/Straight_up_tree/Straight_up_tree.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>
	
	<model name="fir" usescaleof="height" scale="2" renderingdistance="200">
		<submodels>
			<submodel mesh="3d_objects/plants/fir/models/fir/fir.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>
	
	<model name="oak" usescaleof="height" renderingdistance="200">
		<submodels>
			<submodel mesh="3d_objects/plants/tree/models/oak_static/old_oak.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>	
	
	<model name="oak_young" usescaleof="height" >
		<submodels>
			<submodel mesh="3d_objects/plants/tree/models/oak_02/oak_02.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>	
	<model name="oak_stump" usescaleof="all" showcontained="true">
		<translate x="0.000000" y="0.000000" z="0.000000" />
		<rotation x="1.000000" y="1.000000" z="1.000000" degrees="0.000000" />
		<submodels>
			<submodel mesh="3d_objects/plants/tree/models/oak_static/old_oak_stump.mesh">
				<parts>
					<part name="main" show="true">
						<subentities>
							<subentity index="0" material="/global/plants/tree/oak" />
							<subentity index="1" material="/global/plants/tree/oak" />
						</subentities>
					</part>
				</parts>
			</submodel>
		</submodels>
		<actions />
	</model>	
	<model name="oak_chopped" usescaleof="all" showcontained="true">
		<translate x="0.000000" y="0.000000" z="0.000000" />
		<rotation x="1.000000" y="1.000000" z="1.000000" degrees="0.000000" />
		<submodels>
			<submodel mesh="3d_objects/plants/tree/models/oak_static/old_oak_chopped.mesh">
				<parts>
					<part name="main" show="true">
						<subentities>
							<subentity index="0" material="/global/plants/tree/oak" />
							<subentity index="1" material="/global/plants/tree/oak" />
							<subentity index="2" material="/global/plants/tree/oak" />
						</subentities>
					</part>
				</parts>
			</submodel>
		</submodels>
		<actions />
