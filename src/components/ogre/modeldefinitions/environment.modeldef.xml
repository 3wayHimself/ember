<?xml version = '1.0' encoding = 'UTF-8'?>
<models>
	<model name="acorn" scale="10">
		<submodels>
			<submodel mesh="3d_objects/plants/fruitable/models/acorn/acorn.mesh" >
			<parts>
				<part name="main" show="true" />
			</parts>
			</submodel>
		</submodels>
	</model>
	
	<model name="fire">
		<ParticleSystems>
			<ParticleSystem script="Examples/Smoke">
				<Bindings>
					<Binding emittervar="emission_rate" atlasattribute="status" />
				</Bindings>
				
			</ParticleSystem>
			
		</ParticleSystems>
	</model>	
	
	<model name="lumber">
		<submodels>
			<submodel mesh="3d_objects/plants/tree/models/log/tree_log.mesh" >
			<parts>
				<part name="main" show="true" />
			</parts>
			</submodel>
		</submodels>
	</model>	
	
	<model name="mushroom" scale="3">
		<submodels>
			<submodel mesh="3d_objects/plants/mushroom/models/round_mushroom/mushroom.mesh">
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>	
	
	<model name="turnip" scale="1" renderingdistance="30" >
		<submodels>
			<submodel mesh="3d_objects/plants/fruitable/models/leafy_round_rooth_growth/leafy_round_rooth_growth.mesh">
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>	
	
	<model name="carrot" scale="1" renderingdistance="30">
		<submodels>
			<submodel mesh="3d_objects/plants/fruitable/models/leafy_long_rooth_growth/leafy_long_rooth_growth.mesh">
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>	
	
	<model name="boulder" usescaleof="all" showcontained="true">
		<translate x="0.000000" y="0.000000" z="0.000000" />
		<rotation x="1.000000" y="0.000000" z="0.000000" degrees="0.000000" />
		<submodels>
			<submodel mesh="3d_objects/environment/boulder/models/mossy_boulders/mossy_bouldersA.mesh">
				<parts>
					<part name="boulders_A" group="main" show="true">
						<subentities>
							<subentity index="0" material="/global/environment/boulder/mossy_boulder" />
						</subentities>
					</part>
				</parts>
			</submodel>
			<submodel mesh="3d_objects/environment/boulder/models/mossy_boulders/mossy_bouldersB.mesh">
				<parts>
					<part name="boulders_B" group="main" show="false">
						<subentities>
							<subentity index="0" material="/global/environment/boulder/mossy_boulder" />
						</subentities>
					</part>
				</parts>
			</submodel>
			<submodel mesh="3d_objects/environment/boulder/models/mossy_boulders/mossy_bouldersC.mesh">
				<parts>
					<part name="boulders_C" group="main" show="false">
						<subentities>
							<subentity index="0" material="/global/environment/boulder/mossy_boulder" />
						</subentities>
					</part>
				</parts>
			</submodel>
			<submodel mesh="3d_objects/environment/boulder/models/mossy_boulders/mossy_bouldersD.mesh">
				<parts>
					<part name="boulders_D" group="main" show="false">
						<subentities>
							<subentity index="0" material="/global/environment/boulder/mossy_boulder" />
						</subentities>
					</part>
				</parts>
			</submodel>
		</submodels>
		<actions />
    </model>
	
	<model name="apple" scale="10">
		<submodels>
			<submodel mesh="3d_objects/plants/fruitable/models/acorn/acorn.mesh" >
				<parts>
						<part name="main" show="true" >
							<subentities>
								<subentity name="top" material="/global/plants/fruitable/green_apple"  />
								<subentity name="bottom" material="/global/plants/fruitable/green_apple"  />
							</subentities>
						</part>  
				</parts>
			</submodel>
		</submodels>
	</model>
	
	<model name="pile">
		<submodels>
			<submodel mesh="3d_objects/environment/ground/models/rounded_heap/rounded_heap.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>
	
	<model name="grass">
		<submodels>
			<submodel mesh="3d_objects/plants/tussock/models/small_plant/simplegrass.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>
	
	<model name="sun">
		<submodels>
			<submodel mesh="3d_objects/environment/sky/models/sphere/sphere.mesh" >
				<parts>
					<part name="main" show="true" />
				</parts>
			</submodel>
		</submodels>
	</model>
	
    <model name="iron_ore_node" usescaleof="all" scale="2.000000" showcontained="true">
