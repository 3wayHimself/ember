material /global/creature/tubular/fish/scaly_brown_white
{
  receive_shadows on
  technique
  {
    pass
    {
         cull_hardware none 
         cull_software none
		alpha_rejection greater_equal 32 
       texture_unit
		{
			texture 3d_objects/creatures/tubular/textures/scaly_brown_white/low/tubular.png
		}

    }
  }
}

