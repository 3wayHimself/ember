material Field/Heartblood
{
  //receive_shadows off
  technique
  {
    pass
    {
          scene_blend alpha_blend 
         cull_hardware none 
         cull_software none
 		alpha_rejection greater_equal 128
       texture_unit
	{
	
		texture 3d_objects/environment/field/textures/heartblood/field.png
	}

	ambient 1 1 1
      diffuse 1.000000 1.000000 1.000000
      //specular 0.500000 0.500000 0.500000 50.000000
      emissive 0.000000 0.000000 0.000000
    }
  }

}


