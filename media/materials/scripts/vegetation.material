material /global/plants/trees/evergreen/fir : /base/normalmap/nonculled/alpharejected
{
	set_texture_alias DiffuseMap 3d_objects/plants/trees/textures/evergreen/firD.png
	set_texture_alias NormalHeightMap 3d_objects/plants/trees/textures/evergreen/firN.png
	technique NormalMapped
	{
		pass Main
		{
			alpha_rejection greater_equal 160
		}
	}
	technique NormalMappedOld
	{
		pass Main
		{
			alpha_rejection greater_equal 160
		}
	}
	technique General
	{
		pass Main
		{
			alpha_rejection greater_equal 160
		}
	}
	technique GeneralDistance
	{
		pass Main
		{
			alpha_rejection greater_equal 160
		}
	}
}
