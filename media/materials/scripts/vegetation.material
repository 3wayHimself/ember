
material /global/plants/tree/fir/branch
{
	receive_shadows off
	technique
	{
		pass
		{
			//scene_blend alpha_blend 
			cull_hardware none 
			cull_software none
			alpha_rejection greater_equal 64
			texture_unit
			{
				texture 3d_objects/plants/fir/textures/common_fir/low/fir_branch_normal_01_hh.png
			}
		}
	}
}

material /global/plants/tree/evergreen/evergreen_fir : /base/simple/nonculled/alpharejected
{
	set_texture_alias DiffuseMap 3d_objects/plants/tree/textures/evergreen/evergreen_fir.png
