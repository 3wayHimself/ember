
material /global/plants/tree/fir/branch
{
  receive_shadows off
  technique
  {
    pass
    {
          scene_blend alpha_blend 
         cull_hardware none 
         cull_software none
		alpha_rejection greater_equal 64
        texture_unit
	{
	
		texture 3d_objects/plants/fir/textures/common_fir/fir_branch_normal_01_hh_128.png
	}

    }
  }
}


material /global/plants/tree/fir/bark
{
  receive_shadows on
  technique
  {
    pass
    {
         texture_unit
	{
		texture 3d_objects/plants/fir/textures/common_fir/bark_small_128.png
	}
    }
  }
}

material /global/plants/tree/oak
{
  receive_shadows off
  technique
  {
    pass
    {
        // scene_blend alpha_blend 
		alpha_rejection greater_equal 64
         cull_hardware none 
         cull_software none
         texture_unit
	{
		texture 3d_objects/plants/tree/textures/gnarly/tree_128.png
//		texture 3d_objects/plants/tree/textures/gnarly/tree.png
	}
      diffuse 1.000000 1.000000 1.000000
    }
  }
}


